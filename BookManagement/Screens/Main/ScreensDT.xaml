    <DataTemplate DataType="{x:Type local:OfflineScreenVM}">
        <local:OfflineScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:HomeScreenVM}">
        <local:HomeScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:SaleBookScreenVM}">
        <local:SaleBookScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:BookManagementScreenVM}">
        <local:BookManagementScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:BillManagementScreenVM}">
        <local:BillManagementScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:DebtManagementScreenVM}">
        <local:DebtManagementScreen/>
